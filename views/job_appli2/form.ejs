              <tr>
                <td>
                  <label for="dob">Date Of Birth : </label>
                  <input type="text" id="dob" class="require date" name="dob" value="" placeholder="yyyy-mm-dd">
                </td>
              </tr>
            </table>
            <br>
          </fieldset>

          <fieldset id="edu_detail" class="field">
