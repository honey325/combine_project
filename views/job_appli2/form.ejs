            </fieldset>
            <fieldset id="tech" class="field">
              <legend>Tchnologies you know</legend>
              <table>
                <tr>
                  <td>
                    <input type="checkbox" onchange="lang_check(event)" name="php[]" value="php" id="php">
                    Php

                    <input type="radio" name="php[]" value="beginer" class="dis" id="php_beginer">
                    Beginer

                    <input type="radio" name="php[]" value="mideator" class="dis" id="php_mideator">
