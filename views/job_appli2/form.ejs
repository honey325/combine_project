          <div class="tech-lang">
            <fieldset id="lang" class="field">
              <legend>Language Known</legend>
              <table>
                <tr>
                  <td>
                    <input type="checkbox" name="hindi[]" onchange="lang_check(event)" value="hindi" class="lang"
